#include <windows.h>

110 DIALOGEX 0, 0, 200, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Enter Password"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT           "Password:", -1, 10, 20, 60, 12
    EDITTEXT        1001, 70, 18, 100, 14, ES_PASSWORD | ES_AUTOHSCROLL
    DEFPUSHBUTTON   "OK", IDOK, 40, 60, 50, 14
    PUSHBUTTON      "Cancel", IDCANCEL, 100, 60, 50, 14
END
